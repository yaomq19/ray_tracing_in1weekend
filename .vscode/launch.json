{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["./output/output_MSAA.ppm"],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/githubRepositories/ray_tracing_in1weekend",
      "program": "d:/githubRepositories/ray_tracing_in1weekend/main.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}